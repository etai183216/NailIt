@model List<NailIt.Models.ReportTable>
@{
	foreach (var item in Model)
	{
	<tbody>
				<tr data-toggle="modal" data-target="#examineModal">
				<th scope="row">@item.ReportId</th>
					<td>@item.ReportPlaceC</td>
				<td>
					@item.ReportResult =="True" )? "待審核" : "審核"
				</td>
				<td>@item.ReportBuildTime</td>
					<td><button data-toggle="modal" data-target="#reviewModal">審 核</button></td>
				</tr>
	</tbody>
		
	}
}